@model IEnumerable<Rieltor.Models.AllPostViewModel>
@using Rieltor.Controllers

@{
    ViewBag.Title = "Post";
}

<h2>Post</h2>
<table class="table">
    <thead>

    </thead>
      <tbody>
          
@foreach(var post in PostController.postList)
{

        <tr>
            <th>@post.Id</th>
            <th>@post.City.Name</th>
            <th>@post.Neigh.Name</th>
            <th>@post.Address.Street</th>
            <th>@post.Address.Number</th>
            <th>@post.AllArea</th>
            <th>@post.LiveArea</th>
            <th>@post.CookArea</th>
            <th>@post.Flor</th>
            <th>@post.AllFlor</th>
            <th>@post.Description</th>
            <th>@post.Price</th>

        </tr>


   


} 
       
     </tbody>

</table>